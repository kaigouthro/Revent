{
  "name": "revents",
  "version": "1.0.0",
  "description": "Event organizing app Revents, build with React, Redux, Semantic-UI & Firebase",
  "main": "index.js",
  "scripts": {
    "clear": "rm -rf ./build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer build/static/js/main.*",
    "deploy": "npm run build && firebase deploy",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint --fix . --ext .js",
    "heroku-prebuild": "npm install --only=dev",
    "heroku-postbuild": "npm run build"
  },
  "engines": {
    "node": "10.5.0",
    "npm": "6.1.0"
  },
  "keywords": [
    "React",
    "Redux",
    "Redux-Router",
    "Semantic-UI-React",
    "Firebase/Firestore"
  ],
  "author": "TrungDT",
  "license": "ISC",
  "dependencies": {
    "cuid": "^2.1.4",
    "date-fns": "^1.29.0",
    "firebase": "4.12.1",
    "google-map-react": "^1.1.0",
    "moment": "^2.22.2",
    "react": "^16.6.3",
    "react-cropper": "^1.0.1",
    "react-datepicker": "^2.0.0",
    "react-dom": "^16.6.3",
    "react-dropzone": "^4.2.10",
    "react-infinite-scroller": "^1.2.2",
    "react-lazyload": "^2.3.0",
    "react-load-script": "^0.0.6",
    "react-loadable": "^5.4.0",
    "react-places-autocomplete": "^6.1.3",
    "react-redux": "^5.1.1",
    "react-redux-firebase": "^2.2.2",
    "react-redux-toastr": "^7.4.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "2.1.1",
    "redux": "^3.7.2",
    "redux-auth-wrapper": "^2.0.3",
    "redux-firestore": "^0.6.0",
    "redux-form": "^7.4.2",
    "redux-thunk": "^2.3.0",
    "revalidate": "^1.2.0",
    "semantic-ui-css": "2.2.14",
    "semantic-ui-react": "^0.80.0"
  },
  "devDependencies": {
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^4.0.1",
    "prettier": "^1.12.1",
    "redux-devtools-extension": "^2.13.2",
    "source-map-explorer": "^1.5.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "eslintIgnore": [
    "node_modules"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
